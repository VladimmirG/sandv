dsArr=new Array(2,3,4,6,7,18,14,0,16,26,28,13,15,8,17,21,29,20,24,12,25,30,31,1,9,19,5,23,27,22,10,11)
function initpro(objthis, objnext,d) {
	for (var i = 0; i < apArr.length; i++) {
		objthis.get(0).options.add(new Option(apArr[i][0]+" "+apArr[i][1],apArr[i][1])); 
		if(i==d) objthis.get(0).options[i].selected = true;
	}
	for (var i = 2; i < apArr[d].length; i++) {
		objnext.get(0).options.add(new Option(apArr[d][i],apArr[d][i]));  
	}
}

function changeit(objthis, objnext) {
	objthis = $(objthis).get(0);
	objnext = $(objnext).get(0);
	if (objthis.selectedIndex == -1) return;
	var tmpArray = apArr[objthis.selectedIndex];
	objnext.options.length = tmpArray.length-2;
	for (var i = 0; i < tmpArray.length-2; i++) {
		objnext.options[i].text = objnext.options[i].value = tmpArray[i+2];
		if(i+2==2)objnext.options[i].selected = true;
	}
}
